<launch>
    <node pkg="joy" type="joy_node" name="joy1" args="" respawn="false">
        <param name="dev" value="/dev/input/js0"/>
        <param name="deadzone" value="0.2"/>
        <param name="autorepeat_rate" value="20"/>
    </node>

    <rosparam file="$(find exceed_bot)/config/joy_gamesir.yaml" command="load"/>
    <!-- <rosparam file="$(find exceed_bot)/config/joy_frsky.yaml" command="load"/> -->

    <node pkg="joy_teleop" type="joy_teleop.py" name="joy_teleop">
    </node>

    <node pkg="exceed_bot" type="motor_control" name="motor_control" output="screen" required="true">
    </node>

</launch>
